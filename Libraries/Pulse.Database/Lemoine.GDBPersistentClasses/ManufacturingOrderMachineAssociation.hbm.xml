<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  namespace="Lemoine.GDBPersistentClasses"
  assembly="Pulse.Database">
  <subclass name="ManufacturingOrderMachineAssociation"
            discriminator-value="ManufacturingOrderMachineAssociation"
            extends="MachineModification, Pulse.Database">
    <join table="manufordermachineassociation"
          fetch="select">
      <key column="modificationid" />
      <many-to-one name="Machine" column="machineid" class="Machine"
                   access="nosetter.camelcase-m-underscore"
                   cascade="none"
                   not-null="true"
                   secondary-key="true"
                   not-found="exception" />
      <property name="Range"
                access="nosetter.camelcase-m-underscore"
                type="Lemoine.NHibernateTypes.UTCDateTimeRangeType, Lemoine.Database"
                column="manufordermachineassociationrange"
                not-null="true" />
      <many-to-one name="ManufacturingOrder" column="manuforderid" class="ManufacturingOrder"
                   cascade="none"
                   not-found="exception" />
      <property name="Option"
                column="manufordermachineassociationoption" />
    </join>
  </subclass>
</hibernate-mapping>
