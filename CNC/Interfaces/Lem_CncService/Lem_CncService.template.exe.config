        <bindingRedirect oldVersion="{Lemoine.Extensions.Business.major}.0.0.0-{Lemoine.Extensions.Business.major}.{Lemoine.Extensions.Business.minor}.99.99" newVersion="{Lemoine.Extensions.Business.version}.0"/>
      </dependentAssembly>
      <dependentAssembly>
