        <bindingRedirect oldVersion="{Lemoine.Extensions.Alert.major}.0.0.0-{Lemoine.Extensions.Alert.major}.{Lemoine.Extensions.Alert.minor}.99.99" newVersion="{Lemoine.Extensions.Alert.version}.0"/>
      </dependentAssembly>
