    </assemblyBinding>
  </runtime>

  <appSettings>
    <add key="Cnc.DataHandler.GarbageCollection.MinMemory" value="209715200" />
    <!-- 200 MB -->
    <!--add key="DoProfile" value="true" /-->
    <!--add key="ProfileThreshold" value="00:00:00.5" /-->
    <!--add key="ProfileLimit" value="100" /-->
  </appSettings>

</configuration>
